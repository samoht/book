(jbuild_version 1)

(rule (
  (targets (classes.html))
  (deps    (classes.md))
  (action (
    run
    pandoc
    --section-divs
    -f
    markdown-smart-auto_identifiers
    -t
    html5
    ${<}
    -o
    ${@}))))

