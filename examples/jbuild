  (deps ((files_recursively_in code/ocp-index)))
  (action (
    chdir code/ocp-index (
      progn
      (run   cram             index_ncurses.sh)
      (diff? index_ncurses.sh index_ncurses.sh.corrected))))))

(alias (
  (name runtest-all)
  (deps ((files_recursively_in code/ocp-index)))
  (action (
    chdir code/ocp-index (
      progn
      (run cram --non-deterministic index_ncurses.sh)
      (diff? index_ncurses.sh index_ncurses.sh.corrected))))))

