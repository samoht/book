(jbuild_version 1)

(alias (
  (name runtest)
  (deps ((files_recursively_in code/async/echo)))
  (action (
    chdir code/async/echo (
      progn
      (run   cram        run_echo.sh)
      (diff? run_echo.sh run_echo.sh.corrected))))))

(alias (
  (name runtest-all)
  (deps ((files_recursively_in code/async/echo)))
  (action (
    chdir code/async/echo (
      progn
      (run cram --non-deterministic run_echo.sh)
      (diff? run_echo.sh run_echo.sh.corrected))))))

